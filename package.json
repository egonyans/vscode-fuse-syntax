{
    "name": "vscode-fuse-syntax",
    "displayName": "Fuse Open language",
    "description": "Code Completion",
    "version": "1.1.1",
    "publisher": "naumovs",
    "engines": {
        "vscode": "^1.34.0"
    },
    "icon": "preview.jpg",
    "main": "./out/src/extension",
    "categories": [
        "Programming  Languages",
        "Linters",
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:uno",
        "onLanguage:unoproj",
        "onLanguage:ux",
        "onCommand:fuse.connect"
    ],
    "contributes": {
        "commands": [
            {
                "command": "fuse.preview.android",
                "title": "Fuse: Preview Android"
            },
            {
                "command": "fuse.preview.ios",
                "title": "Fuse: Preview iOS"
            },
            {
                "command": "fuse.preview.local",
                "title": "Fuse: Preview local"
            },
            {
                "command": "fuse.preview.local.debug",
                "title": "Fuse: Debug local"
            },
            {
                "command": "fuse.connect",
                "title": "Fuse: Connect to daemon"
            }
        ],
        "languages": [
            {
                "id": "unoproj",
                "aliases": [],
                "extensions": [
                    ".unoproj"
                ]
            },
            {
                "id": "uno",
                "aliases": [
                    "Uno",
                    "uno"
                ],
                "extensions": [
                    ".uno",
                    ".unotrace"
                ],
                "configuration": "./languages/uno.json"
            },
            {
                "id": "ux",
                "aliases": [
                    "UX"
                ],
                "extensions": [
                    ".ux"
                ],
                "configuration": "./languages/ux.json"
            }
        ],
        "grammars": [
            {
                "language": "uno",
                "scopeName": "source.uno",
                "path": "./syntax/uno.tmLanguage"
            },
            {
                "language": "ux",
                "scopeName": "text.xml",
                "path": "./syntax/ux.tmLanguage"
            },
            {
                "language": "unoproj",
                "scopeName": "text.unoproj",
                "path": "./syntax/unoproj.tmLanguage"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sergiirocks/vscode-fuse-syntax.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sergiirocks/vscode-fuse-syntax/issues"
    },
    "homepage": "https://github.com/sergiirocks/vscode-fuse-syntax#readme",
    "dependencies": {},
    "scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"@types/vscode": "^1.34.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1"
	}
}
